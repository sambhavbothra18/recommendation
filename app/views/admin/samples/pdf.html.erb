<h1>User List PDF</h1>
<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>FullName</th>
      <th>Status</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.fullname %></td>
        <td><%= user.status %></td>
      </tr>
    <% end %>
  </tbody>
</table>
