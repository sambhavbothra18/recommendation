<h1>Create Profile</h1>
<%= form_with model: @profiles do |f| %>
  <div>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
  </div>
    
  <div>
    <%= f.label :bio %>
    <%= f.text_field :bio %><br>
  </div>
    
  <div>  
    <%= f.label :profile_picture %>
    <%= f.file_field :profile_picture %><br>
  </div>  

  <div>
    <%= f.submit 'Save' %>
  </div>  
<% end %>